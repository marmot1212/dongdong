<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.administrator.vegetarians824.fragment.MapFragment">

    <!-- TODO: Update blank fragment layout -->
    <FrameLayout
        android:id="@+id/rells"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        >
        <com.amap.api.maps2d.MapView
            android:clickable="true"
            android:id="@+id/mishimap"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
        </com.amap.api.maps2d.MapView>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:orientation="vertical"
            android:background="@drawable/button_bg_stroke1"
            android:translationZ="5dp"
            android:elevation="5dp">
            <LinearLayout
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginTop="5dp"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:background="@drawable/button_bg3"
                android:orientation="horizontal">
                <ImageView
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:src="@drawable/findgray"
                    android:padding="8dp"/>
                <LinearLayout
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:focusableInTouchMode="true"
                    android:focusable="false">
                        <TextView
                            android:id="@+id/ditu_edit"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_marginLeft="5dp"
                            android:hint="查找餐厅、活动、酒店"
                            android:paddingBottom="5dp"
                            android:paddingTop="5dp"
                            android:textColorHint="#c2c2c2"/>
                </LinearLayout>
                <TextView
                    android:layout_gravity="right"
                    android:id="@+id/mishi_city"
                    android:clickable="true"
                    android:layout_width="60dp"
                    android:layout_height="match_parent"
                    android:text="北京市"
                    android:lines="2"
                    android:textSize="12sp"
                    android:gravity="center"
                    android:textColor="#00aff0"/>
            </LinearLayout>
        </LinearLayout>
        <ImageView
            android:layout_marginLeft="10dp"
            android:id="@+id/mishidingwei"
            android:scaleType="fitXY"
            android:layout_marginBottom="70dp"
            android:layout_gravity="bottom|left"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:src="@drawable/ditu_userlocation_2"/>
        <ImageView
            android:layout_marginLeft="10dp"
            android:clickable="true"
            android:id="@+id/mishfind"
            android:scaleType="fitXY"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_gravity="left|bottom"
            android:layout_marginBottom="120dp"/>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom"
            android:elevation="10dp"
            >
            <LinearLayout
                android:visibility="invisible"
                android:id="@+id/ditu_find_lin_liebiao"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#cef2f2f2"
                android:gravity="center"
                android:orientation="vertical"
                >

                <ImageView
                    android:layout_marginTop="2dp"
                    android:layout_marginBottom="2dp"
                    android:layout_width="60dp"
                    android:layout_height="15dp"
                    android:src="@drawable/ditu_list"
                    />
                <TextView
                    android:id="@+id/ditu_find_tv_liebiao"
                    android:layout_marginBottom="5dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="列出结果"
                    android:textSize="12sp" />
            </LinearLayout>
            <LinearLayout
                android:layout_below="@+id/ditu_find_lin_liebiao"
                android:id="@+id/mishi_bottom"
                android:layout_width="match_parent"
                android:orientation="vertical"
                android:layout_height="0dp">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:background="#fff">
                    <LinearLayout
                        android:id="@+id/mishi_choose"
                        android:layout_weight="0"
                        android:layout_width="0dp"
                        android:layout_height="match_parent">
                        <TextView
                            android:gravity="center"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:text="类型"/>
                    </LinearLayout>
                    <LinearLayout
                        android:id="@+id/mishi_rank1"
                        android:layout_weight="1"
                        android:layout_width="0dp"
                        android:layout_height="40dp"
                        android:orientation="horizontal"
                        android:gravity="center">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="距离"/>
                        <ImageView
                            android:id="@+id/mishi_rankima0"
                            android:layout_marginLeft="5dp"
                            android:layout_width="20dp"
                            android:layout_height="8dp"
                            android:src="@drawable/barrow"/>
                    </LinearLayout>
                    <LinearLayout
                        android:id="@+id/mishi_rank2"
                        android:layout_weight="1"
                        android:layout_width="0dp"
                        android:gravity="center"
                        android:layout_height="match_parent">
                        <TextView
                            android:layout_gravity="center"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="价格"/>
                        <ImageView
                            android:visibility="invisible"
                            android:id="@+id/mishi_rankima"
                            android:layout_marginLeft="5dp"
                            android:layout_width="20dp"
                            android:layout_height="8dp"
                            android:src="@drawable/barrow"/>
                    </LinearLayout>
                </LinearLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="#e2e2e2">
                </View>
                <ListView
                    android:id="@+id/candan_listview"
                    android:focusableInTouchMode="true"
                    android:filterTouchesWhenObscured="true"
                    android:touchscreenBlocksFocus="true"
                    android:layout_width="match_parent"
                    android:background="#fff"
                    android:layout_height="match_parent">
                </ListView>
            </LinearLayout>
        </RelativeLayout>

        <FrameLayout
            android:visibility="invisible"
            android:id="@+id/mishimsg"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginTop="45dp">
            <LinearLayout
                android:layout_marginTop="5dp"
                android:layout_marginLeft="25dp"
                android:layout_width="match_parent"
                android:layout_height="30dp">
                <FrameLayout
                    android:id="@+id/msg_line"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/button_bg_raduis">
                    <TextView
                        android:id="@+id/msg_text"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="25dp"
                        android:textSize="12sp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="暂无本地信息收录，欢迎上传"
                        android:textColor="#fff"/>
                    <LinearLayout
                        android:id="@+id/msg_close"
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:layout_gravity="right">
                        <ImageView
                            android:layout_marginLeft="10dp"
                            android:layout_marginTop="10dp"
                            android:layout_width="10dp"
                            android:layout_height="10dp"
                            android:src="@drawable/close3" />
                    </LinearLayout>
                </FrameLayout>
            </LinearLayout>
            <ImageView
                android:id="@+id/msg_ima"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:src="@drawable/msg"/>

        </FrameLayout>
    </FrameLayout>

</LinearLayout>
