<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    tools:context="com.example.administrator.vegetarians824.mine.MySetting">
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="#00aff0">
        <LinearLayout
            android:layout_marginLeft="5dp"
            android:layout_width="50dp"
            android:layout_height="match_parent"
            android:id="@+id/my_set_fanhui"
            android:clickable="true"
            >
            <ImageView
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_gravity="center_vertical"
                android:src="@drawable/back_white" />
        </LinearLayout>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="设置"
            android:textColor="#fff"
            android:textSize="16sp"
            android:gravity="center"/>
        <ImageView
            android:id="@+id/share_dongdong"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:src="@drawable/share2"
            android:layout_gravity="right|center_vertical"
            android:layout_marginRight="15dp"/>
    </FrameLayout>
    <FrameLayout
        android:layout_marginTop="15dp"
        android:background="#fff"
        android:layout_width="match_parent"
        android:layout_height="40dp">
        <TextView
            android:layout_marginLeft="20dp"
            android:layout_gravity="center_vertical"
            android:textSize="16sp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="推送通知"/>
        <Switch
            android:layout_marginRight="20dp"
            android:layout_gravity="right|center_vertical"
            android:id="@+id/set_switch"
            android:checked="true"
            android:shadowColor="#00aff0"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

    </FrameLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#f2f2f2">
    </View>
    <FrameLayout
        android:id="@+id/set_fankui"
        android:background="#fff"
        android:layout_width="match_parent"
        android:layout_height="40dp">
        <TextView
            android:layout_marginLeft="20dp"
            android:layout_gravity="center_vertical"
            android:textSize="16sp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="反馈"/>
        <ImageView
            android:layout_width="15dp"
            android:layout_height="20dp"
            android:src="@drawable/next"
            android:layout_gravity="center_vertical|right"
            android:layout_marginRight="20dp"/>
    </FrameLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#f2f2f2">
    </View>
    <FrameLayout
        android:id="@+id/set_huancun"
        android:background="#fff"
        android:layout_width="match_parent"
        android:layout_height="40dp">
        <TextView
            android:layout_marginLeft="20dp"
            android:layout_gravity="center_vertical"
            android:textSize="16sp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="清除缓存"/>
        <ImageView
            android:layout_width="15dp"
            android:layout_height="20dp"
            android:src="@drawable/next"
            android:layout_gravity="center_vertical|right"
            android:layout_marginRight="20dp"/>
    </FrameLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#f2f2f2">
    </View>
    <FrameLayout
        android:id="@+id/set_updata"
        android:background="#fff"
        android:layout_width="match_parent"
        android:layout_height="40dp">
        <TextView
            android:layout_marginLeft="20dp"
            android:layout_gravity="center_vertical"
            android:textSize="16sp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="版本更新"/>
        <ImageView
            android:layout_width="15dp"
            android:layout_height="20dp"
            android:src="@drawable/next"
            android:layout_gravity="center_vertical|right"
            android:layout_marginRight="20dp"/>
    </FrameLayout>
    <FrameLayout
        android:id="@+id/set_ganxie"
        android:background="#fff"
        android:layout_marginTop="10dp"
        android:layout_width="match_parent"
        android:layout_height="40dp">
        <TextView
            android:layout_marginLeft="20dp"
            android:layout_gravity="center_vertical"
            android:textSize="16sp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="感谢有你"/>
        <ImageView
            android:layout_width="15dp"
            android:layout_height="20dp"
            android:src="@drawable/next"
            android:layout_gravity="center_vertical|right"
            android:layout_marginRight="20dp"/>
    </FrameLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    <LinearLayout
        android:layout_marginBottom="60dp"
        android:gravity="center"
        android:layout_alignParentBottom="true"
        android:layout_width="match_parent"
        android:layout_height="80dp">
        <ImageView
            android:layout_marginRight="30dp"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:src="@drawable/applogo"/>
        <ImageView
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:src="@drawable/logo002"/>
    </LinearLayout>
        <TextView
            android:id="@+id/set_vesion"
            android:textSize="12sp"
            android:layout_marginBottom="30dp"
            android:layout_alignParentBottom="true"
            android:layout_width="match_parent"
            android:gravity="center"
            android:layout_height="wrap_content"
            android:text="咚咚 V2.2.2  2015-2016 素活项目组"/>
    </RelativeLayout>
</LinearLayout>
